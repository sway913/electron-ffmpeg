{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/common/**/*",
    "src/constants/**/*",
    "src/interfaces/**/*",
    "src/utils/**/*",
    "src/renderer/**/*",
    "typings/electronPreLoad.d.ts",
    "typings/env.d.ts",
  ],
  "compilerOptions": {
    "composite": true,
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@siteMain/*": [
        "src/renderer/siteMain/src/*"
      ],
      
    }
  }
}
